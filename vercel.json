{
  "version": 2,
  "builds": [
    {
      "src": "/server.ts",
      "use": "@vercel/node"
    },
    {
      "src": "/dist/VersoMart/browser/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "dest": "/server.ts"
    },
    {
      "src": "/(.*\\..+)$",
      "dest": "/dist/VersoMart/browser/$1"
    },
    {
      "src": "/.*",
      "dest": "/server.ts"
    }
  ]
}
